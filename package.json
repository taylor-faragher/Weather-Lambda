    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "lambda-local": "^2.2.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.100.0",
